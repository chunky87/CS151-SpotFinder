<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml"
      fx:controller="edu.sjsu.cs.SpotFinder.PlaceController" style="-fx-background-color: #ADD8E6;">

    <ScrollPane fx:id="scrollPane" fitToWidth = "true" style="-fx-background-color: #ADD8E6;">
        <VBox fx:id="rootVBox" alignment="CENTER" style="-fx-background-color: #ADD8E6; -fx-padding: 40 0 80 0;">
            <Label fx:id = "firstText" text="This is the Results Page!" style="-fx-font-size: 28; -fx-font-family: 'Courier New'; -fx-font-weight: BOLDER; -fx-alignment: CENTER"/>
            <Label fx:id = "description" text="(Showing top 10 results within 5000 meters)" style="-fx-font-size: 12; -fx-font-family: 'Courier New'" />
            <Label fx:id="name1" text="" style="-fx-font-size: 16; -fx-alignment: BASELINE_LEFT" />
            <Label text="" style="-fx-padding: 0 0 50 0" />
            <Button text="NEW SEARCH" onAction="#goBackToHomepage" style="-fx-font-size: 16; -fx-alignment: CENTER" styleClass="search-button"/>
        </VBox>
    </ScrollPane>
</VBox>